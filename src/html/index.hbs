<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<!-- smoosh -->
	<link rel='stylesheet' href='bundle.css' />
	<!-- endsmoosh -->

	<!-- boilerplate.css -->

	{{> base/font}}

	<!-- smoosh -->
	<script src='critical.js'></script>
	<!-- endsmoosh -->

	{{> base/analytics }}

</head>

<body class='tk-netflix'>

	{{> base/header 'sticky'}}

	<main id='content'>
		<figure id='chart'>
			<div class='chart__curate_purp'></div>
			<canvas class='canvas__bg'></canvas>
			<canvas class='canvas__fg'></canvas>
			<canvas class='canvas__ex'></canvas>
			<canvas class='canvas__globe'></canvas>
			<div class='chart__curate'></div>
			<div class='chart__overlay'></div>
			<svg class='chart__svg'>
				<g>
					<g class='g-globe is-hidden'></g>
					<g class='g-nodes'></g>
					<g class='g-voronoi'></g>
				</g>
			</svg>
			<img class='airplane' src='assets/images/airplane.png' alt='airplane'>
			<div class='chart__tweets'></div>
		</figure>
		<section data-index='0' id='intro'>
			{{> story/intro }}
		</section>
		<section data-index='1' id='globe'>
			{{> story/globe }}
		</section>
		<section data-index='2' id='curate'>
			{{> story/curate }}
		</section>
		<section data-index='3' id='explore'>
			{{> story/explore }}
		</section>
		<section data-index='4' id='outro'>
			{{> story/outro }}
		</section>

	</main>

 <!-- <div class='test'>
		@@include('dots-purple-01.svg')
	</div> -->

	<script src='assets/scripts/d3.v4.12.0+jetpack.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>
