<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<!-- smoosh -->
	<link rel='stylesheet' href='bundle.css' />
	<!-- endsmoosh -->

	<!-- boilerplate.css -->

	{{> base/font}}

	<!-- smoosh -->
	<script src='critical.js'></script>
	<!-- endsmoosh -->

	{{> base/analytics }}

</head>

<body class='tk-netflix'>

	{{> base/header 'sticky'}}

	<main id='content'>
		<figure id='chart'>
			<svg class='chart__svg'>
				<g>
					<g class='g-map is-hidden'></g>
					<g class='g-nodes'></g>
				</g>
			</svg>
			<div class='chart__tweets'></div>
		</figure>
		<section data-index='0' id='intro'>
			{{> story/intro }}
		</section>
		<section data-index='1' id='map'>
			{{> story/map }}
		</section>
		<section data-index='2' id='curate'>
			{{> story/curate }}
		</section>
		<section data-index='3' id='explore'>
			{{> story/explore }}
		</section>
		<section data-index='4' id='outro'>
			{{> story/outro }}
		</section>

	</main>

	<script src='assets/scripts/d3.v4.12.0+jetpack.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>
