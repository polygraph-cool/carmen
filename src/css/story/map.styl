#chart .g-map
	display none
	.graticule
		fill none
		stroke $gray-light
		stroke-dasharray 4px 4px
		display none

	.border
		fill none
		stroke $gray-light

	.land
		stroke $gray-light
		fill $off-white
		fill-opacity 0.1

	.outline
		fill none
		stroke $bg
		stroke-width 3px
		// display none

	.sphere
		fill $off-black

	.country
		fill $off-white
		fill-opacity 0
